{
    "compilerOptions": {
        "lib": ["es2015", "es2017"],
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "target": "es5",
        "allowJs": true,
        "outDir": "./build"
    }
}